<div id="footer" class="container">
  <div class="row">
    <div id="footer_left" class="span4">
      <p><%= application_name %></p>
    </div>
    <div id="footer_center" class="span4"></div>
    <div id="footer_right" class="span4">
      <ul id="footer-links" class="inline">
        <li class="footer-link">
          <%= link_to t('blacklight.pages.about.title'), about_path %>
        </li>
        <li class="footer-link">
          <%= link_to t('blacklight.pages.contact.title'), feedback_path %>
        </li>
      </ul>
    </div>
  </div>
</div>

<%= debug(params) if Rails.env.development? %>